{
  "name": "mj-react-form-builder",
  "versizon": "2.0.26",
  "description": "A React form builder package using Tailwind CSS",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "yarn build-cjs && yarn build-esm",
    "build-cjs": "rimraf ./dist/cjs && tsc --outDir dist/cjs",
    "build-esm": "rimraf ./dist/esm && tsc --outDir dist/esm --module es2015",
    "release": "npm run build && npm version patch && git push --follow-tags && npm publish"
  },
  "files": [
    "dist/**/*",
    "index.css"
  ],
  "keywords": [
    "react",
    "form",
    "builder",
    "typescript"
  ],
  "author": "Marcos Jimenez",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@tsconfig/node16": "^16.1.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.0",
    "@mui/material": "^6.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "getenv": "^1.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-number-format": "^5.4.1",
    "tailwindcss": "^3.4.10"
  },
  "packageManager": "yarn@1.22.22",
  "version": "0.0.51"
}
